<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.ezd.dao.IndustryDao">

    <!--通过<resultMap>映射实体类属性名和表的字段名对应关系 -->

    <resultMap id="industry" type="Industry">
        <!-- 用id属性来映射主键字段 -->
        <id property="industryId" column="industryId"></id>
        <!-- 用result属性来映射非主键字段 -->
        <result property="industryName" column="industryName" />
        <!--  一对多关联 -->
        <collection property="enterprises" resultMap="edu.ezd.dao.EnterpriseDao.enterprise" ></collection>
        <collection property="positionNames" resultMap="edu.ezd.dao.PositionNameDao.positionName" ></collection>
    </resultMap>

</mapper>
